    border-right-color: rgb(0, 206, 255);
    border-top-color: rgb(0, 206, 255);
    border-bottom-color: rgb(0, 206, 255);
    background-color: rgb(255, 209, 0);
    transition-duration: 0.2s;
}

.cycleContainer {
    align-items: center;
    align-self: stretch;
    flex-direction: row;
    justify-content: space-between;
    background-color: rgba(0, 113, 140, 0.28);
    -unity-background-scale-mode: stretch-to-fill;
    -unity-slice-left: 0;
    -unity-slice-top: 0;
    -unity-slice-right: 0;
    -unity-slice-bottom: 0;
    margin-top: 5px;
    margin-bottom: 5px;
}

.cycleButton:hover {
    background-color: rgb(0, 206, 255);
}

.unity-progress-bar__progress {
